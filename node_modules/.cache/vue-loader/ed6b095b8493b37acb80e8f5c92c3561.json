n        width: 35px;\n    }\n    .el-submenu .el-menu-item {\n        min-width: 180px;\n    }\n\n    .hiddenDropdown,\n    .hiddenDropname {\n        display: none;\n    }\n    .fa {\n        text-decoration: none;\n    }\n\n    .el-dropdown-link {\n        cursor: pointer;\n        color: #409EFF;\n    }\n    .el-icon-arrow-down {\n        font-size: 12px;\n    }\n    .demonstration {\n        display: block;\n        color: #8492a6;\n        font-size: 14px;\n        margin-bottom: 20px;\n    }\n</style>\n\n<script>\n    export default {\n        name:'left-manu',\n        data(){\n            return {\n                items:[\n                    {\n                        icon:'fa-money',\n                        name:'资产管理',\n                        path:'fund',\n                        children:[\n                            {\n                                path:'foundlist',\n                                name:'腾讯云资产详情'\n                            },\n                            {\n                                path:'assetlist',\n                                name:'公司网络资产详情'\n                            },\n                            {\n                                path:'serverlist',\n                                name:'公司服务器资产详情'\n                            },\n                            {\n                                path:'auditlist',\n                                name:'cvm申请详情'\n                            }\n                        ]\n                    },\n                    {\n                        icon:'fa-pencil',\n                        name:'应用管理',\n                        path:'pod',\n                        children:[\n                            {\n                                path:'podlist',\n                                name:'应用管理详情'\n                            }\n                        ]\n                    }\n                ]\n            }\n        }\n    }\n</script>\n"]}]}